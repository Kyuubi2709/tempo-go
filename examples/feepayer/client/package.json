{
  "name": "tempo-go-server-typescript-client",
  "version": "1.0.0",
  "description": "TypeScript client for Go Tempo fee payer server",
  "type": "module",
  "scripts": {
    "start": "tsx client.ts",
    "simple": "tsx simple.ts",
    "debug": "tsx debug.ts",
    "dev": "tsx watch client.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "check": "npm run lint && npm run format:check"
  },
  "dependencies": {
    "@noble/curves": "^2.0.1",
    "dotenv": "^16.3.1",
    "tempo.ts": "^0.4.2",
    "viem": "^2.21.54"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
